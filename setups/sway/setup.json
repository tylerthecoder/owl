{
  "dependencies": [
    "waybar",
    "mako",
    "cliphist",
    "swww"
  ],
  "install": "local:install.sh",
  "links": [
    {
      "source": "local:config",
      "target": "~/.config/sway/config"
    },
    {
      "source": "local:scripts/desk-sway",
      "target": "~/.local/bin/desk-sway"
    },
    {
      "source": "local:scripts/desks/framework-sway.sh",
      "target": "~/.config/desks-sway/framework-sway.sh"
    },
    {
      "source": "local:scripts/desks/home-sway.sh",
      "target": "~/.config/desks-sway/home-sway.sh"
    },
    {
      "source": "local:scripts/desks/redwood-sway.sh",
      "target": "~/.config/desks-sway/redwood-sway.sh"
    },
    {
      "source": "local:environment.d/99-environment.conf",
      "target": "~/.config/environment.d/99-environment.conf"
    }
  ],
  "menu_scripts": [
    "local:scripts/desk-sway"
  ],
  "rc_scripts": [
    "local:alias.sh"
  ],
  "name": "sway"
}

